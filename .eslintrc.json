// .eslintrc.json

{
    "extends": ["airbnb-typescript/base"],
    "env": {
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": 12
    },
    "rules": {
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "import/extensions": ["error", "never"]
    }
  }
  